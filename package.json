		"@types/estraverse": "^5.1.2",
		"esquery": "^1.4.0",
		"estraverse": "^5.3.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.3",
		"vite": "^4.1.0"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	}
}
